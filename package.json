{
  "name": "@rbxts/syn-utils",
  "version": "0.1.0",
  "description": "A utilities package that I use for personal development but is welcomed to be used, if you find yourself with any use cases :)",
  "repository": "https://github.com/dev-syn/rbxts-syn-utils.git",
  "author": "dev-syn",
  "license": "MIT",
  "main": "out/init.luau",
  "typings": "out/index.d.ts",
  "files": [
    "out/**/*.lua",
    "out/**/*.d.ts",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rbxtsc --type package",
		"build:test": "rbxtsc --type game --rojo test.project.json",
    "watch": "rbxtsc -w --type package",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/types": "^1.0.867",
    "rbxts-transform-debug": "^2.2.0",
    "roblox-ts": "^3.0.0",
		"typescript": "=5.5.3"
  },
  "dependencies": {
    "@flamework/components": "^1.3.1",
    "@flamework/core": "^1.3.1",
    "@rbxts/t": "^3.2.1",
    "rbxts-transformer-flamework": "^1.3.1",
    "typedoc": "^0.28.9"
  },
	"overrides": {
		"typescript": "$typescript"
	}
}
