{
  "name": "@rbxts/syn-utils",
  "version": "0.1.0",
  "description": "A utilities package that I use for personal development but is welcomed to be used, if you find yourself with any use cases :)",
  "repository": "https://github.com/dev-syn/rbxts-syn-utils.git",
  "author": "dev-syn",
  "license": "MIT",
  "main": "out/init.luau",
  "typings": "out/index.d.ts",
  "files": [
    "out/**/*.lua",
    "out/**/*.d.ts",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rbxtsc --type package",
    "watch": "rbxtsc -w --type package",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^3.0.0-types.0",
    "@rbxts/types": "^1.0.867",
    "roblox-ts": "^3.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "typedoc": "^0.28.9"
  }
}
